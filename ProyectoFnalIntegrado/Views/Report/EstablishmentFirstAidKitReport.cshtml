@model IEnumerable<dynamic>

<table class="table">
    <thead>
        <tr>
            <th>Fecha de Registro</th>
            <th>ID del Botiquín</th>
            <th>Nombre del Botiquín</th>
            <th>Nombre de la Escuela</th>
            <th>ID del Medicamento</th>
            <th>Nombre del Medicamento</th>
            <th>Cantidad</th>
            <th>Fecha de Expiración</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var medicine in Model)
        {
            <tr>
                <td>@medicine.RegisterDate</td>
                <td>@medicine.FirstAidKitId</td>
                <td>@medicine.FirstAidKitName</td>
                <td>@medicine.SchoolName</td>
                <td>@medicine.MedicineId</td>
                <td>@medicine.MedicineName</td>
                <td>@medicine.Quantity</td>
                <td>@medicine.ExpirationDate.ToString("yyyy-MM-dd")</td>
            </tr>
        }
    </tbody>
</table>
